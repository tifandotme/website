{
  "headers": [
    {
      "source": "/:all*(ttf|otf|woff|woff2)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/gh",
      "destination": "https://github.com/tifandotme"
    },
    {
      "source": "/gh/:slug*",
      "destination": "https://github.com/tifandotme/:slug*"
    }
  ]
}
